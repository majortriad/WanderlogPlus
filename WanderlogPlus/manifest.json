{
    "manifest_version": 2,
    "name": "Wanderlog Plus",
    "version": "0.1",
    "description": "Enhances features of Wanderlog.com",
    "icons": {
        "48": "icons/wlplus-48.png",
        "96": "icons/wlplus-96.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.wanderlog.com/plan/*"
            ],
            "js": [
                "wanderlogplus.js"
            ]
        }
    ],
    "permissions": [
        "https://openflights.org/php/alsearch.php"
    ]
}